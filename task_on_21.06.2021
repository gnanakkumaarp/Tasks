// reverse a string

var s = reverseString("JavaScript");
console.log(s);
function reverseString(s)
{
    let temp = "";
   for (let i = s.length - 1; i >= 0; i--) {
        temp += s[i];
   }
   return temp;
}

function mergeArrays(arr1, arr2) {
    return [...arr1,...arr2];
}

