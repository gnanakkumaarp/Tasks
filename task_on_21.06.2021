// printPrimeNumbers

function printPrimeNumbers(n) {
    let temp = [];
    for (let i = 1; i <= n; i++) {
        if (i % 2 !== 0) {
            temp.push(i);// console.log(i);
        }
    }
    return temp;
}
console.log(printPrimeNumbers(100));

function isPrime(n) {
    if (n % 2 == 0) {
