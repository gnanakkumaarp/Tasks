console.log(sumCSV('1.5, 2.4, 3.1, 4, 5.5, 6, 7, 8, 9, 10.9'));
function sumCSV(s)
{
  let sum = 0;
  let arr = s.split(",").map(Number);
  for (let i = 0; i < arr.length; i++) {
        sum += i;
  }
  return sum;
}


